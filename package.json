{
  "name": "star_river",
  "private": true,
  "version": "0.0.0",
  "author": "Jabori",
  "description": "Star River App",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "concurrently \"vite\" \"cross-env NODE_ENV=development electron .\"",
    "start": "vite",
    "lint": "eslint .",
    "preview": "vite preview",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.jabori.starriver",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  },
